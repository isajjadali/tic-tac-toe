{
    "specialization": {},
    "middleware": {
        "static": {
            "module": {
                "arguments": [
                    "path:./../public"
                ]
            }
        },
        "router": {
            "module": {
                "arguments": [
                    {
                        "directory": "path:./controllers"
                    }
                ]
            }
        },
        "pagination": {
            "enabled": false,
            "priority": 200,
            "route": "/api",
            "module": {
                "name": "path:./middlewares/pagination"
            }
        },
        "appsec": {
            "priority": 110,
            "module": {
                "name": "lusca",
                "arguments": [
                    {
                        "csrf": false
                    }
                ]
            }
        }
    },
    "app": {}
}
